@model List<AaravEnterprise.ViewModel.UserOrderViewModel>
@{
    ViewData["Title"] = "InCompleted Order";
    List<Order> objOrder = ViewBag.UserOrder;
}
<div class="container">
    <div class="headings">
        <h2 class="heading">InCompleted Orders</h2>
    </div>
    <div class="row">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone Number</th>
                    <th scope="col">Order Amount</th>
                    <th scope="col" colspan="2">Action</th>
                    
                </tr>
            </thead>
            <tbody>
                @foreach (var userOrder in Model)
                {
                <tr>
                    <td scope="row"> @userOrder.CustName</td>
                    <td scope="row"> @userOrder.CustEmail</td>
                    <td scope="row"> @userOrder.CustPhoneNumber</td>
                    <td scope="row"> @userOrder.TotalAmount</td>
                    <td scope="row" colspan="2">
                    <a asp-controller="ViewInvoice" asp-action="Index" asp-route-Id="@userOrder.InvoiceId"  class="btn btn-success btn-xs"><i class="fa fa-eye"></i></a>&nbsp;|&nbsp;<a onclick="return confirm('Do you want to delete this record?')" asp-action="Delete" asp-controller="ViewInvoice" asp-route-Id=""  class="btn btn-danger btn-xs"><i class="fa fa-trash"></i></a>
                    </td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>